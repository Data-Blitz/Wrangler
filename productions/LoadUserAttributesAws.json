{
  "productions": [
    {
      "logger": {
        "loggerImpl": "./loggers/WinstonLogger.js",
        "configuration": {
          "options": {
            "timestamp": true,
            "depth": true,
            "prettyPrint": true
          },
          "filename":"/Users/paul/clients/bpf-data-blitz-wrangler/WranglerLog.json"
        }
      },


      "source": {
        "name": "DynamoLogStream",
        "sourceImpl": "./sources/DynamoToStream.js",
        "configuration": {
          "filename": "/Users/paul/data-blitz/clients/bpf/workspaces/awsLoadUserContent/examples/UserAttribute/UserAttribute-1-2018-04-03-00-46-57-539d66fe-db13-4c9e-9e93-76a1eae3d0c3.json",
          "batchSize": 50,
          "maxNumberOfOutstandingBatches": 4,
          "backPressureWaitInMilliseconds": 30
        },
        "spout": {
          "name": "TupleStreamSpout",
          "spoutImpl": "./spouts/ToTupleStream.js"
        }
      },
      "wranglers": [
        {
          "name": "AddTimestamp",
          "wranglerImpl": "./wranglers/AddTimestamp.js",
          "configuration": {
            "timestampAttributeName": "timestamp-ISO",
            "format": "ISO"
          }
        },
        {
          "name": "TranslateFromDynamo",
          "wranglerImpl": "./wranglers/TranslateFromDynamo.js",
          "configuration": {
          }
        }
      ],
      "sink": {
        "name": "ElasticSearch",
        "sinkImpl": "./sinks/ElasticSearchSink.js",
        "configuration": {
          "host": "localhost:9200",
          "sinkIndex": "aws-users-attributes",
          "sinkType": "attribute",
          "_idAttributeName": "instanceid",
          "bulkAction": "index"
        }
      }
    }
  ]
}
