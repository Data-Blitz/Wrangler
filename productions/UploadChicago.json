{
  "productions": [
    {
      "name": "UpLoadPlatformEvents",
      "logger": {
        "loggerImpl": "./loggers/WinstonLogger.js",
        "configuration": {
          "options": {
            "timestamp": true,
            "depth": true,
            "prettyPrint": true
          }
        }
      },
      "source": {
        "name": "CsvBatchTupleStream",
        "sourceImpl": "./sources/CsvFileToStream.js",
        "configuration": {
          "filename": "/Users/pharvener/Downloads/Food_Inspections.csv",
          "batchSize": 1000,
          "maxNumberOfOutstandingBatches": 20,
          "backPressureWaitInMilliseconds": 3
        },
        "spout": {
          "name": "TupleStreamSpout",
          "spoutImpl": "./spouts/ToTupleStream.js"
        }
      },
      "wranglers": [
        {
          "name": "AddTimestamp",
          "wranglerImpl": "./wranglers/AddTimestamp.js",
          "configuration": {
            "timestampAttributeName": "timestamp-ISO",
            "format": "ISO"
          }
        }
      ],
      "sink": {
        "name": "ElasticSearch",
        "sinkImpl": "./sinks/ElasticSearchSink.js",
        "configuration": {
          "host": "localhost:9200",
          "sinkIndex": "chicago-food-inspections",
          "sinkType": "inspection",
          "_idAttributeName": "inspection id",
          "bulkAction": "index"
        }
      }
    }
  ]
}
